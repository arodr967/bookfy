      <JFXTextField fx:id="EmailAddressTextField" labelFloat="true" layoutX="46.0" layoutY="206.0" prefHeight="29.0" prefWidth="500.0" promptText="Email Address" />
      <JFXTextField fx:id="AddressLineTextField" labelFloat="true" layoutX="47.0" layoutY="290.0" prefHeight="29.0" prefWidth="500.0" promptText="Address Line:" />
      <JFXTextField fx:id="cityTextField" labelFloat="true" layoutX="47.0" layoutY="341.0" prefHeight="29.0" prefWidth="211.0" promptText="City:" />
      <JFXTextField fx:id="StateTextField" labelFloat="true" layoutX="282.0" layoutY="341.0" prefHeight="29.0" prefWidth="68.0" promptText="State:" />
